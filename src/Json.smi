_require "basis.smi"
_require "./Base.smi"
_require "./Jansson.smi"

structure Json = struct
  datatype t
    = True
    | False
    | Null
    | String of string
    | Integer of int
    | Real of real
    | Object of (string * t) list
    | Array of t list

  val parseString : string -> t

  val toString : t -> string
end
